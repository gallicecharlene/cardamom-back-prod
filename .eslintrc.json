{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions" : "off",
        "indent": ["error", 4],
        "max-len": "off",
        "no-console": "off",
        "prefer-destructuring": "off"
    }
}